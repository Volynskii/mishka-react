    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "date-fns": "^2.16.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
