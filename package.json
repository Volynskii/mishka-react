    "react-router-dom":"^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.3",
    "@types/react-redux": "^7.1.14",
